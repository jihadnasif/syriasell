# ios/Podfile

platform :ios, '13.0'

# تحميل البيئة الخاصة بـ Flutter
# Podfile بدون استخدام ephemeral
# Flutter iOS pods
target 'Runner' do
  use_frameworks!
  use_modular_headers!

  install_all_flutter_pods(File.dirname(File.realpath(__FILE__)))
end

target 'Runner' do
  use_frameworks!
  use_modular_headers!

  # تثبيت جميع بودات Flutter
  install_all_flutter_pods(File.dirname(File.realpath(__FILE__)))
end
